<p id="notice"><%= notice %></p>


<p>
  Number of members:
  <%= @beer_club.members.count%>
</p>

<p>
  <strong>Name:</strong>
  <%= @beer_club.name %>
</p>

<p>
  <strong>Founded:</strong>
  <%= @beer_club.founded %>
</p>

<p>
  <strong>City:</strong>
  <%= @beer_club.city %>
</p>


<p>
  <% @beer_club.members.each do |member| %>
    <%= member.to_s %>
  <% end %>  
</p>

<%= link_to('Edit', edit_beer_club_path(@beer_club)) if current_user %> |
<%= link_to 'Back', beer_clubs_path %>
